    },
    "_opening_left": {
      "type": "STRING",
      "value": "#(|"
    },
    "_closing_left": {
      "type": "STRING",
      "value": ")#"
    },
    "left": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
