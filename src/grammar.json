{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "helix",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "primary_left"
          },
          {
            "type": "SYMBOL",
            "name": "primary_right"
          },
          {
            "type": "SYMBOL",
            "name": "right"
          },
          {
            "type": "SYMBOL",
            "name": "left"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          },
          {
            "type": "SYMBOL",
            "name": "text_primary"
          }
        ]
      }
    },
    "text_primary": {
      "type": "PATTERN",
      "value": "[^#\\[\\]\\|]+"
    },
    "opening_primary_left": {
      "type": "STRING",
      "value": "#[|"
    },
    "closing_primary_left": {
      "type": "STRING",
      "value": "]#"
    },
    "primary_left": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "opening_primary_left"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "text_primary"
          }
        },
        {
          "type": "SYMBOL",
          "name": "closing_primary_left"
        }
      ]
    },
    "opening_primary_right": {
      "type": "STRING",
      "value": "#["
    },
    "closing_primary_right": {
      "type": "STRING",
      "value": "|]#"
    },
    "primary_right": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "opening_primary_right"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "text_primary"
          }
        },
        {
          "type": "SYMBOL",
          "name": "closing_primary_right"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[^#\\(\\)\\|]+"
    },
    "opening_right": {
      "type": "STRING",
      "value": "#("
    },
    "closing_right": {
      "type": "STRING",
      "value": "|)#"
    },
    "right": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "opening_right"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "SYMBOL",
          "name": "closing_right"
        }
      ]
    },
    "opening_left": {
      "type": "STRING",
      "value": "#(|"
    },
    "closing_left": {
      "type": "STRING",
      "value": ")#"
    },
    "left": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "opening_left"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "SYMBOL",
          "name": "closing_left"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
