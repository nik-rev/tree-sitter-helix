{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "helix",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "left_primary"
          },
          {
            "type": "SYMBOL",
            "name": "right_primary"
          },
          {
            "type": "SYMBOL",
            "name": "right"
          },
          {
            "type": "SYMBOL",
            "name": "left"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "start_right_primary": {
      "type": "STRING",
      "value": "#["
    },
    "end_right_primary": {
      "type": "STRING",
      "value": "|]#"
    },
    "right_primary": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "start_right_primary"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "char_primary"
            },
            "named": true,
            "value": "char"
          }
        },
        {
          "type": "SYMBOL",
          "name": "cursor_primary"
        },
        {
          "type": "SYMBOL",
          "name": "end_right_primary"
        }
      ]
    },
    "char": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "char_primary"
        },
        {
          "type": "SYMBOL",
          "name": "char_regular"
        }
      ]
    },
    "_text_primary": {
      "type": "PATTERN",
      "value": "[^#\\[\\]\\|]+"
    },
    "char_primary": {
      "type": "PATTERN",
      "value": "[^#\\[\\]\\|]"
    },
    "char_regular": {
      "type": "PATTERN",
      "value": "[^#\\(\\)\\|]"
    },
    "cursor_primary": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "char_primary"
      },
      "named": false,
      "value": "cursor_primary"
    },
    "cursor": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "char_regular"
      },
      "named": false,
      "value": "cursor"
    },
    "start_left_primary": {
      "type": "STRING",
      "value": "#[|"
    },
    "end_left_primary": {
      "type": "STRING",
      "value": "]#"
    },
    "left_primary": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "start_left_primary"
        },
        {
          "type": "SYMBOL",
          "name": "cursor_primary"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "char_primary"
            },
            "named": true,
            "value": "char"
          }
        },
        {
          "type": "SYMBOL",
          "name": "end_left_primary"
        }
      ]
    },
    "text": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_text"
        },
        {
          "type": "SYMBOL",
          "name": "_text_primary"
        }
      ]
    },
    "_text": {
      "type": "PATTERN",
      "value": "[^#\\(\\)\\|]+"
    },
    "start_right": {
      "type": "STRING",
      "value": "#("
    },
    "end_right": {
      "type": "STRING",
      "value": "|)#"
    },
    "right": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "start_right"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "char_regular"
            },
            "named": true,
            "value": "char"
          }
        },
        {
          "type": "SYMBOL",
          "name": "cursor"
        },
        {
          "type": "SYMBOL",
          "name": "end_right"
        }
      ]
    },
    "start_left": {
      "type": "STRING",
      "value": "#(|"
    },
    "end_left": {
      "type": "STRING",
      "value": ")#"
    },
    "left": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "start_left"
        },
        {
          "type": "SYMBOL",
          "name": "cursor"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "char_regular"
            },
            "named": true,
            "value": "char"
          }
        },
        {
          "type": "SYMBOL",
          "name": "end_left"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
