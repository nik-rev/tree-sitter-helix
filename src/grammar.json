{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "helix",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "primary_left"
          },
          {
            "type": "SYMBOL",
            "name": "primary_right"
          },
          {
            "type": "SYMBOL",
            "name": "right"
          },
          {
            "type": "SYMBOL",
            "name": "left"
